{
  "name": "markdown-it-pfm",
  "version": "0.1.3",
  "description": "Markdown It preset for Pandoc Fravored Markdown",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src/index.js --out-file dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/tani/markdown-it-pfm"
  },
  "keywords": [
    "pandoc",
    "markdown",
    "markdown-it",
    "markdown-it-plugin"
  ],
  "author": "TANIGUCHI Masaya",
  "license": "MIT",
  "babel": {
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "babel-plugin-add-module-exports"
    ]
  },
  "dependencies": {
    "markdown-it-attrs": "^4.1.3",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-container": "^3.0.0",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-gridtables": "^0.3.0",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-mathjax3": "^4.2.2",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "babel-plugin-add-module-exports": "^1.0.4"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/index.js"
  ]
}
